{
	"name": "mood-tracker",
	"version": "1.0.0",
	"description": "Mood tracking application",
	"main": "server.js",
	"scripts": {
		"start": "node --optimize_for_size --max_old_space_size=460 server.js",
		"server": "cd server && npm start",
		"develop": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
		"install": "cd server && npm i && cd ../client && npm i",
		"build": "npm run seed && cd client && npm run build",
		"seed": "cd server && npm run seed",
		"client": "cd client && npm start"
	},
	"devDependencies": {
		"concurrently": "^5.1.0",
		"nodemon": "^2.0.19"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ElsieMay/mood-tracker.git"
	},
	"author": "Elsie Lawrie",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/ElsieMay/mood-tracker/issues"
	},
	"homepage": "https://github.com/ElsieMay/mood-tracker#readme",
	"dependencies": {
		"colors": "^1.4.0",
		"cors": "^2.8.5",
		"dotenv": "^16.0.1",
		"express": "^4.18.1",
		"mongoose": "^6.4.6",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-hook-form": "^7.34.0",
		"if-env": "^1.0.4"
	}
}
